<xsd:pattern value="[0-9a-f]{8}-?([0-9a-f]{4}-?){3}-?[0-9a-f]{12}"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:complexType>
  <xsd:complexType name="StudentLoanInterestPaidType">
	<xsd:annotation>
		<xsd:documentation>Student Loan Interst Paid Information</xsd:documentation>
	    
	</xsd:annotation>
	<xsd:sequence>
		<xsd:element name="haveStudentLoanInterestPP">
			<xsd:annotation>
				<xsd:documentation>
					<Description>mef++ : Did filer pay student loan interest?</Description>
				</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="BooleanType">
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:element>
	</xsd:sequence>	
<xsd:attribute name="uuid">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:pattern value="[0-9a-f]{8}-?([0-9a-f]{4}-?){3}-?[0-9a-f]{12}"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:complexType>
  <xsd:complexType name="PersonalPropertyRentalExpensesType">
    <xsd:annotation>
        <xsd:documentation>Personal Property Rental Expenses values for taxp and sp to be included in total for Form 1040, line 32F</xsd:documentation>
        
    </xsd:annotation>
    <xsd:sequence>
        <xsd:element name="TaxpayerPersonalPropertyRentalExpensePP" type="USAmountType">
            <xsd:annotation>
                <xsd:documentation>PPR Expenses paid by the taxpayer</xsd:documentation>
            </xsd:annotation>
        </xsd:element>
        <xsd:element name="SpousePersonalPropertyRentalExpensePP" type="USAmountType">
            <xsd:annotation>
                <xsd:documentation>PPR Expenses paid by the spouse</xsd:documentation>
            </xsd:annotation>
        </xsd:element>
    </xsd:sequence>
<xsd:attribute name="uuid">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:pattern value="[0-9a-f]{8}-?([0-9a-f]{4}-?){3}-?[0-9a-f]{12}"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:complexType>
  <xsd:complexType name="WagesSalariesAndTipsWorksheetType">
      <xsd:annotation>
         <xsd:documentation>Other wages earned to be included in the total for Form 1040, line 7</xsd:documentation>
         
      </xsd:annotation>
      <xsd:sequence>
          <xsd:element name="TaxpayerWagesSalariesAndTipsGrpPP" type="WagesSalariesAndTipsGroupType">
            <xsd:annotation>
               <xsd:documentation>
                  <Description>mef++ : wages, salaries, and tips data for taxpayer</Description>
               </xsd:documentation>
            </xsd:annotation>
         </xsd:element>

          <xsd:element name="SpouseWagesSalariesAndTipsGrpPP" type="WagesSalariesAndTipsGroupType">
              <xsd:annotation>
                  <xsd:documentation>
                      <Description>mef++ : wages, salaries, and tips data for spouse</Description>
                  </xsd:documentation>
              </xsd:annotation>
          </xsd:element>
          
          <xsd:element name="TotalWagesSalariesAndTipsGrpPP" type="WagesSalariesAndTipsGroupType">
              <xsd:annotation>
                  <xsd:documentation>
                      <Description>mef++ : total wages, salaries, and tips data for both taxpayer and spouse</Description>
                  </xsd:documentation>
              </xsd:annotation>
          </xsd:element>
      </xsd:sequence>
   <xsd:attribute name="uuid">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:pattern value="[0-9a-f]{8}-?([0-9a-f]{4}-?){3}-?[0-9a-f]{12}"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:complexType>
  <xsd:complexType name="WagesSalariesAndTipsGroupType">
        <xsd:annotation>
            <xsd:documentation>Other wages earned to be included in the total for Form 1040, line 7.  
            There is one WagesSalariesAndTipesGroup type for taxpayer, one for spouse, 
            and one that is the total of taxpayer and spouse.
            </xsd:documentation>
            
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="W2WagesAmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : total W2 wages</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="MiscIncomeFrom8819AmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : misc income from form 8819</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="DisabilityBeforeMinRetirementAge1099RAmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : Disability before minimum retirement age from form 1099-R</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="ReturnOfContrib1099RAmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : return of contributions form 1099-R</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="ExcessReimbursement2106AmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : Excess Reiumbursement, form 2106</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="TaxableTips4137AmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : Taxable tips, form 4137</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="NonCashTipsAmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : Noncash tips</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="ExcessMovingExpenseReimbursement3903AmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : Excess moving expense reimbursement from form 3903</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="NonW2HouseholdEmployeeWagesAmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : Wages earned as a household employee (if less than threshold and without a Form W-2)</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="SickPayOrDisabilityAmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : Sick pay or disability not reported on W-2 or 1099-R</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="TotalForeignSourceIncomeAmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : Total foreign source income not reported on W-2 or 1099-R</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="NonW2StockOrdinaryIncomeAmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : Ordinary income from employer stock transactions not reported on W-2</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element maxOccurs="unbounded" name="OtherEarnedIncomeGrpPP">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : Total of other earned income sources</Description>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="DescriptionPP" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <Description>mef++ : Description of the other earned income </Description>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="OtherEarnedIncomeAmtPP" type="USAmountType">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <Description>mef++ : the amount of other earned income </Description>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                <xsd:attribute name="uuid">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:pattern value="[0-9a-f]{8}-?([0-9a-f]{4}-?){3}-?[0-9a-f]{12}"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:complexType>
            </xsd:element>
            
            
            <xsd:element name="TotalOtherEarnedIncomeAmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : Total of other earned income sources</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="TotalEarnedIncomePP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : Total earned income</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="EmployerProvidedDependentCareAmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : taxable employer-provided dependent care benefits</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="EmployerProvidedAdoptionBenefitsAmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : taxable employer-provided adoption benefits less any excluded benefits from Form 8839</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="ScholarshipFellowshipNotOnW2AmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : Scholarship/fellowship income not on form W-2</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <!-- like other earned income, there needs to be a way to include the line items for other non-earned income here
                but reference a description in the single WagesSalariesAndTipsWorksheetType-->
            
            <xsd:element name="TotalOtherNonEarnedIncomeAmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : total of other non earned-income</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            
            <xsd:element name="TotalWagesSalariesAndTipsAmtPP" type="USAmountType">
                <xsd:annotation>
                    <xsd:documentation>
                        <Description>mef++ : total wages salaries and tips</Description>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    <xsd:attribute name="uuid">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:pattern value="[0-9a-f]{8}-?([0-9a-f]{4}-?){3}-?[0-9a-f]{12}"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:complexType>
</xsd:schema>
