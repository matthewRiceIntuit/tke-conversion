<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.irs.gov/efile" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.irs.gov/efile" version="1.0">
  <xsd:include schemaLocation="../../Common/HumanDetailPP.xsd"/>
	
	<xsd:annotation>
		<xsd:documentation>
			<Description>IRS e-file Schema - IRS Form 1040</Description>
			<TaxYear>2014</TaxYear>
			<MaturityLevel>Final Release</MaturityLevel>
			<ReleaseDate>December 23, 2014</ReleaseDate>
		</xsd:documentation>
	</xsd:annotation>

	<xsd:include schemaLocation="../../../Common/efileTypes.xsd"/>

	<!-- ================================================================== -->
	<!-- ======================= IRS Form 1040 ============================ -->
	<!-- ================================================================== -->

	<xsd:element name="IRS1040">
		<xsd:annotation>
			<xsd:documentation>IRS Form 1040</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS1040Type">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute fixed="IRS1040" name="documentName" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="referenceDocumentId" type="IdListType">
						<xsd:annotation>
							<xsd:documentation>List of document ID's of forms, schedules, supporting info. etc. attached to this form</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute fixed="IRS8379 IRS8867 IRS8958 BackupWithholdingFromSchedulesK1Statement BinaryAttachment Chapter11BankruptcyStatement Elect1141110gFlowThruEntStatement Form1098DifferenceStatement NameChangeStatement OverseasExtensionStatement PenaltiesStatement SpecialDepreciationAllowanceElectionOutStatement SpouseSignatureStatement GeneralDependencyMedium GeneralDependencySmall" name="referenceDocumentName" type="StringType">
						<xsd:annotation>
							<xsd:documentation>
								<ELFFieldNumber>1329</ELFFieldNumber>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="IRS1040Type">
		<xsd:annotation>
			<xsd:documentation>Content model for IRS Form 1040</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
		
			<!-- Special Condition Description -->
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="SpecialConditionDesc">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Special condition description</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Primary Date of Death -->
			<xsd:element minOccurs="0" name="PrimaryDeathDt" type="DateType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Primary date of death</Description>
						<ELFFieldNumber>0020</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Spouse Date Of Death -->
			<xsd:element minOccurs="0" name="SpouseDeathDt" type="DateType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Spouse date of death</Description>
						<ELFFieldNumber>0040</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<xsd:element minOccurs="0" name="Choice_SpecialProcessingLiteralCd_CombatZoneCd_SpecialProcessingCodeTxt_PP">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:enumeration value="SpecialProcessingLiteralCd"/>
<xsd:enumeration value="CombatZoneCd"/>
<xsd:enumeration value="SpecialProcessingCodeTxt"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:element>
<xsd:choice minOccurs="0">
				<!-- Special Processing Literal -->
				<xsd:element name="SpecialProcessingLiteralCd">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Special processing literal</Description>
							<ELFFieldNumber>0100</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="TextType">
							<xsd:enumeration value="DESERT STORM"/>
							<xsd:enumeration value="HAITI"/>
							<xsd:enumeration value="FORMER YUGOSLAVIA"/>
							<xsd:enumeration value="UN OPERATION"/>
							<xsd:enumeration value="JOINT GUARD"/>
							<xsd:enumeration value="JOINT FORGE"/>
							<xsd:enumeration value="NORTHERN WATCH"/>
							<xsd:enumeration value="OPERATION ALLIED FORCE"/>
							<xsd:enumeration value="NORTHERN FORGE"/>
							<xsd:enumeration value="ENDURING FREEDOM"/>
							<xsd:enumeration value="IRAQI FREEDOM"/>
							<xsd:enumeration value="KOSOVO OPERATION"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element fixed="COMBAT ZONE" name="CombatZoneCd">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Special processing literal</Description>
							<ELFFieldNumber>0100</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="TextType">
								<xsd:attribute name="combatZoneDt" use="required">
									<xsd:annotation>
										<xsd:documentation>
											<ELFFieldNumber>0100</ELFFieldNumber>
										</xsd:documentation>
									</xsd:annotation>
									<xsd:simpleType>
										<xsd:restriction base="DateType">
											<xsd:pattern value="[1-9][0-9]{3}\-.*"/>
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:attribute>
							<xsd:attribute name="uuid">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:pattern value="[0-9a-f]{8}-?([0-9a-f]{4}-?){3}-?[0-9a-f]{12}"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<!-- Special Processing Code Text -->
				<xsd:element name="SpecialProcessingCodeTxt">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Special processing code text</Description>
							<ELFFieldNumber>0100</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="TextType">
							<xsd:maxLength value="22"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:choice>
			
			<!-- PECF Primary Indicator  -->
			<xsd:element minOccurs="0" name="PECFPrimaryInd" type="CheckboxType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>PECF primary indicator</Description>
						<ELFFieldNumber>0110</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- PECF Spouse Indicator -->
			<xsd:element minOccurs="0" name="PECFSpouseInd" type="CheckboxType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>PECF spouse indicator</Description>
						<ELFFieldNumber>0120</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- =============== Filing Status =============== -->
			<!-- Individual Return Filing Status Cd -->
			<xsd:element name="IndividualReturnFilingStatusCd">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Individual return filing status cd</Description>
						<LineNumber>1 thru 5</LineNumber>
						<ELFFieldNumber>0130</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
						<xsd:enumeration value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<xsd:element minOccurs="0" name="Choice_SpouseNm_NRALiteralCd_PP">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:enumeration value="SpouseNm"/>
<xsd:enumeration value="NRALiteralCd"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:element>
<xsd:choice minOccurs="0">
				<!-- Spouses Name -->
				<xsd:element minOccurs="0" name="SpouseNm" type="PersonNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Spouses name</Description>
							<LineNumber>3</LineNumber>
							<ELFFieldNumber>0140</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<!-- Non-Resident Alien literal code -->
				<xsd:element minOccurs="0" name="NRALiteralCd">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Non-Resident Alien literal code</Description>
							<LineNumber>3</LineNumber>
							<ELFFieldNumber>0140</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="TextType">
							<xsd:enumeration value="NRA"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>	
			</xsd:choice>

			<!-- Head of Household Qualifying Child Name -->
			<xsd:element minOccurs="0" name="QualifyingHOHNm" type="PersonNameType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Head of household qualifying child name</Description>
						<LineNumber>4</LineNumber>
						<ELFFieldNumber>0150</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Head of Household Qualifying Child SSN -->
			<xsd:element minOccurs="0" name="QualifyingHOHSSN" type="SSNType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Head of household qualifying child SSN</Description>
						<LineNumber>4</LineNumber>
						<ELFFieldNumber>0153</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- =============== Exemptions =============== -->
			<!-- Exempt Primary Indicator -->
			<xsd:element minOccurs="0" name="ExemptPrimaryInd" type="CheckboxType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Exempt primary indicator</Description>
						<LineNumber>6a</LineNumber>
						<ELFFieldNumber>0160</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Exempt Spouse Indicator -->
			<xsd:element minOccurs="0" name="ExemptSpouseInd" type="CheckboxType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Exempt spouse indicator</Description>
						<LineNumber>6b</LineNumber>
						<ELFFieldNumber>0163</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<xsd:element minOccurs="0" name="ExemptSpouseNm" type="PersonNameType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Exempt spouse name</Description>
						<LineNumber>6b</LineNumber>
						<ELFFieldNumber>0164</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Exempt Spouse Name Control -->
			<xsd:element minOccurs="0" name="ExemptSpouseNameControlTxt" type="PersonNameControlType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Exempt spouse name control</Description>
						<LineNumber>6b</LineNumber>
						<ELFFieldNumber>0165</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Total Exempt Primary And Spouse Count -->
			<xsd:element minOccurs="0" name="TotalExemptPrimaryAndSpouseCnt">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total exempt primary and spouse count</Description>
						<LineNumber>6</LineNumber>
						<ELFFieldNumber>0167</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="IntegerNNType">
						<xsd:totalDigits value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Dependent Information Repeating Group -->
			<xsd:element maxOccurs="100" minOccurs="0" name="DependentDetail">
				<xsd:complexType>
					<xsd:sequence>
						<!-- Dependent First Name -->
						<xsd:element name="DependentFirstNm" type="PersonFirstNameType">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Dependent first name</Description>
									<LineNumber>6c(1)</LineNumber>
									<ELFFieldNumber>0170</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<!-- Dependent Last Name -->
						<xsd:element name="DependentLastNm" type="PersonLastNameType">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Dependent last name</Description>
									<LineNumber>6c(1)</LineNumber>
									<ELFFieldNumber>0171</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<!-- Dependent Name Control -->
						<xsd:element name="DependentNameControlTxt" type="PersonNameControlType">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Dependent name control</Description>
									<LineNumber>6c</LineNumber>
									<ELFFieldNumber>0172</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<!-- Dependent SSN or "DiedLiteralCd" -->
						<xsd:element minOccurs="0" name="Choice_DependentSSN_DiedLiteralCd_PP">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:enumeration value="DependentSSN"/>
<xsd:enumeration value="DiedLiteralCd"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:element>
<xsd:choice>
							<xsd:element name="DependentSSN" type="SSNType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Dependent SSN</Description>
										<LineNumber>6c(2)</LineNumber>
										<ELFFieldNumber>0175</ELFFieldNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="DiedLiteralCd">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Died Literal Code</Description>
										<LineNumber>6c(2)</LineNumber>
										<ELFFieldNumber>0175</ELFFieldNumber>
									</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="TextType">
											<xsd:enumeration value="DIED"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:choice>
						<!-- Dependent Relationship -->
						<xsd:element name="DependentRelationshipCd">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Dependent relationship</Description>
									<LineNumber>6c(3)</LineNumber>
									<ELFFieldNumber>0177</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="TextType">
									<xsd:enumeration value="SON"/>
									<xsd:enumeration value="DAUGHTER"/>
									<xsd:enumeration value="STEPCHILD"/>
									<xsd:enumeration value="FOSTER CHILD"/>
									<xsd:enumeration value="BROTHER"/>
									<xsd:enumeration value="SISTER"/>
									<xsd:enumeration value="STEPBROTHER"/>
									<xsd:enumeration value="STEPSISTER"/>
									<xsd:enumeration value="HALF BROTHER"/>
									<xsd:enumeration value="HALF SISTER"/>
									<xsd:enumeration value="GRANDCHILD"/>
									<xsd:enumeration value="NIECE"/>
									<xsd:enumeration value="NEPHEW"/>
									<xsd:enumeration value="PARENT"/>
									<xsd:enumeration value="GRANDPARENT"/>
									<xsd:enumeration value="AUNT"/>
									<xsd:enumeration value="UNCLE"/>
									<xsd:enumeration value="OTHER"/>
									<xsd:enumeration value="NONE"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<!-- Eligibile for Child Tax Credit Indicator -->
						<xsd:element minOccurs="0" name="EligibleForChildTaxCreditInd" type="CheckboxType">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Eligibile for child tax credit indicator</Description>
									<LineNumber>6c(4)</LineNumber>
									<ELFFieldNumber>0178</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					<xsd:element name="EICCodePP" type="EICCodeType">
        <xsd:annotation>
            <xsd:documentation>
                <xsd:description>mef++ : EIC Code (Result)</xsd:description>
            </xsd:documentation>
            
        </xsd:annotation>
    </xsd:element>
            <xsd:element name="DependentCodePP" type="DependentCodeType">
        <xsd:annotation>
            <xsd:documentation>
                <xsd:description>mef++ : Dependent Code (Result)</xsd:description>
            </xsd:documentation>
            
        </xsd:annotation>
    </xsd:element>
            <xsd:element name="DependentLegalParentsPP" type="DependentLegalParentsType">
        <xsd:annotation>
            <xsd:documentation>
                <xsd:description>mef++ : Legal Parent of the dependent</xsd:description>
            </xsd:documentation>
            
        </xsd:annotation>
    </xsd:element>
            <xsd:element minOccurs="0" name="DependentBirthDtPP" type="DateType">
        <xsd:annotation>
            <xsd:documentation>
                <xsd:description>mef++ : This dependent's date of birth</xsd:description>
            </xsd:documentation>
            
        </xsd:annotation>
    </xsd:element>
            <xsd:element minOccurs="0" name="DependentDeathDtPP" type="DateType">
        <xsd:annotation>
            <xsd:documentation>
                <xsd:description>mef++ : This dependent's date of death</xsd:description>
            </xsd:documentation>
            
        </xsd:annotation>
    </xsd:element>
            <xsd:element name="DependentAgePP" type="IntegerPosType">
        <xsd:annotation>
            <xsd:documentation>
                <xsd:description>mef++ : This dependent's Age. If not present should be calculated from the DateOfBirth</xsd:description>
            </xsd:documentation>
            
        </xsd:annotation>
    </xsd:element>
          </xsd:sequence>
				<xsd:attribute name="uuid">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:pattern value="[0-9a-f]{8}-?([0-9a-f]{4}-?){3}-?[0-9a-f]{12}"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:complexType>
			</xsd:element>
			
			<!-- Number Of Other Dependents Listed Count Ind -->
			<xsd:element minOccurs="0" name="MoreDependentsInd" type="CheckboxType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>More than four dependents</Description>
						<LineNumber>6c</LineNumber>
						<ELFFieldNumber>0209</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Number of Children Who Lived with You Count -->
			<xsd:element minOccurs="0" name="ChldWhoLivedWithYouCnt">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of children who lived with you count</Description>
						<LineNumber>6c</LineNumber>
						<ELFFieldNumber>0240</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="IntegerNNType">
						<xsd:totalDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Number of Children Not living With You Count-->
			<xsd:element minOccurs="0" name="ChldNotLivingWithYouCnt">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of children not living with you count</Description>
						<LineNumber>6c</LineNumber>
						<ELFFieldNumber>0247</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="IntegerNNType">
						<xsd:totalDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Number of Other Dependents Listed Count -->
			<xsd:element minOccurs="0" name="OtherDependentsListedCnt">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of other dependents listed count</Description>
						<LineNumber>6c</LineNumber>
						<ELFFieldNumber>0350</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="IntegerNNType">
						<xsd:totalDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Total Exemptions Count -->
			<xsd:element minOccurs="0" name="TotalExemptionsCnt">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total exemptions count</Description>
						<LineNumber>6d</LineNumber>
						<ELFFieldNumber>0355</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="IntegerNNType">
						<xsd:totalDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- =============== Income =============== -->			
			<!-- Wages Salaries And Tips Amount -->
			<xsd:element minOccurs="0" name="WagesSalariesAndTipsAmt">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Wages salaries and tips amount</Description>
						<LineNumber>7</LineNumber>
						<ELFFieldNumber>0375</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="wagesNotShownLitOnlyCd">
								<xsd:annotation>
									<xsd:documentation>
										<ELFFieldNumber>0370 0371</ELFFieldNumber>
									</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="TextType">
										<xsd:enumeration value="FB"/>
										<xsd:enumeration value="DCB"/>
										<xsd:enumeration value="FB AND DCB"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute fixed="IRSW2 NonW2DisabilityPaymentStatement WagesNotShownSchedule" name="referenceDocumentName" type="StringType">
								<xsd:annotation>
									<xsd:documentation>
										<ELFFieldNumber>0356 0357 0358 0359 0360 0361 0362 0363 0366 0367 0368 0369 0370 0371 0372 0373 0378 0379</ELFFieldNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
						<xsd:attribute name="uuid">
<xsd:simpleType>
<xsd:restriction base="xsd:string">
<xsd:pattern value="[0-9a-f]{8}-?([0-9a-f]{4}-?){3}-?[0-9a-f]{12}"/>
</xsd:restriction>
</xsd:simpleType>
</xsd:attribute>
</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			
			<!-- Taxable Interest Amount -->
			<xsd:element minOccurs="0" name="TaxableInterestAmt">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable interest amount</Description>
						<LineNumber>8a</LineNumber>
						<ELFFieldNumber>0380</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute fixed="IRS1040ScheduleB" name="referenceDocumentName" type="StringType">
								<xsd:annotation>
									<xsd:documentation>
										<ELFFieldNumber>NL</ELFFieldNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
						<xsd:attribute name="uuid">
<xsd:simpleType>
